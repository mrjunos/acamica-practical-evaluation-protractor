{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "moduleResolution": "node",
    "sourceMap": false,
    "declaration": false,
    "removeComments": false,
    "noImplicitAny": false,
    "outDir": "compiledJs",
    "typeRoots": [ "./node_modules/@types" ],
    "types": ["node", "jasmine"]
  },
  "exclude": [
    "node_modules",
    "compiledJs"
  ]
}